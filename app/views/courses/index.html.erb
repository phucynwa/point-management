<div class="container">
  <div class="col-md-8 col-md-offset-2">
    <table class="table table-striped">
      <thead>
        <tr>
          <th>Course</th>
          <th>Lecture</th>
          <th>Semester</th>
          <th>School Year</th>
          <th>Teacher</th>
          <th>Action</th>
        </tr>
      </thead>
      <tbody>
        <%= render @courses %>
      </tbody>
    </table>
    <%= paginate @courses, theme: "twitter-bootstrap-3" %>
  </div>
</div>
