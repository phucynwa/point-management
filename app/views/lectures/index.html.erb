<div class="container">
  <div class="col-md-3">
    <%= render "layouts/brand" %>
  </div>
  <div class="col-md-9">
    <h3>All Lecture</h3>
    <table class="table table-striped">
      <thead>
        <tr>
          <th>#</th>
          <th>Lecture</th>
          <th>Credits No.</th>
          <th>Courses Quantity</th>
        </tr>
      </thead>
      <tbody>
        <%= render @lectures %>
      </tbody>
    </table>
    <%= paginate @lectures, theme: "twitter-bootstrap-3" %>
  </div>
</div>
